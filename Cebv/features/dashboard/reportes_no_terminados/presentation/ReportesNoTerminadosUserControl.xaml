<UserControl x:Class="Cebv.features.dashboard.reportes_no_terminados.presentation.ReportesNoTerminadosUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Cebv.features.dashboard.reportes_no_terminados.presentation"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <local:ReportesNoTerminadosViewModel/>
    </UserControl.DataContext>
    
    <Grid>
        <DataGrid ItemsSource="{Binding Reportes.data}"/>
    </Grid>
</UserControl>
