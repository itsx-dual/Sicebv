                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap"
                                            Text="{Binding DescripcionOcupacionPrincipal, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Descripcion Ocupacion secundaria"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap" 
