                             <StackPanel Margin="0,8,20,0">
                                 <ui:TextBlock Text="Url boletin"/>
                                 <ui:TextBox Width="225" Text="{Binding UrlBoletin, Mode=TwoWay}"/>
                             </StackPanel>
                             
                             <StackPanel Margin="0,8,0,0">
                                 <ui:TextBlock Text="Boletin imagen"/>
                                 <ui:TextBox Width="225" Text="{Binding BoletinImgPath, Mode=TwoWay}"/>
