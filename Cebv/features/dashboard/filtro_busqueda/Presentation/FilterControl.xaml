                            <StackPanel Margin="0 8 20 0">
                                <ui:TextBlock Text="Pertenencia:" />
                                <ComboBox Width="225"
                                          SelectedItem="{Binding PerteneciaSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Pertenencias}" />
