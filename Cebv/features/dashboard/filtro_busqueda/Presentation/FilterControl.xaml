                                <ui:TextBlock Text="Relacion con la persona &#x0a;Desaparecida o no localizada:"/>
                                <ComboBox Width="225" SelectedItem="{Binding RelacionSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Relaciones}" IsEditable="True"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Marca"/>
                                <ComboBox Width="225" SelectedItem="{Binding MarcaSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Marcas}" IsEditable="True"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Submarca"/>
                                <ui:TextBox Width="225" Text="{Binding Submarca, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorVehiculoSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Colores}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Placa"/>
                                <ui:TextBox Width="225" Text="{Binding Placa, Mode=TwoWay}"/>
                                <ui:TextBlock Text="Error"
                                              FontTypography="Caption"
                                              Visibility="Hidden" />
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Modelo (Año)"/>
                                <ui:TextBox Width="225" Text="{Binding Modelo, Mode=TwoWay}"/>
                                <ui:TextBlock Text="Error"
                                              FontTypography="Caption"
                                              Visibility="Hidden" />
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Numero de serie"/>
                                <ui:TextBox Width="225" Text="{Binding NumeroSerie, Mode=TwoWay}"/>
                                <ui:TextBlock Text="Error"
                                              FontTypography="Caption"
                                              Visibility="Hidden" />
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Numero de motor"/>
                                <ui:TextBox Width="225" Text="{Binding NumeroMotor, Mode=TwoWay}"/>
                                <ui:TextBlock Text="Error"
                                              FontTypography="Caption"
                                              Visibility="Hidden" />
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Númeroo de permiso"/>
                                <ui:TextBox Width="225" Text="{Binding NumeroPermiso, Mode=TwoWay}"/>
                                <ui:TextBlock Text="Error"
                                              FontTypography="Caption"
                                              Visibility="Hidden" />
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Tipo Vehiculo"/>
                                <ComboBox Width="225" SelectedItem="{Binding TipoVehiculoSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding TiposVehiculos}" IsEditable="True"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Uso vehiculo"/>
                                <ComboBox Width="225" SelectedItem="{Binding UsoVehiculoSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding UsosVehiculos}" IsEditable="True"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Vehiculo localizado"/>
                                <ComboBox SelectedValue="{Binding Localizado, Mode=TwoWay}"
                                          DisplayMemberPath="Key"
                                          SelectedValuePath="Value"
                                          ItemsSource="{Binding Opciones}"/>
                            </StackPanel>
                        </WrapPanel>
                        <StackPanel Grid.Row="1" Margin="0,8,0,0" Orientation="Vertical" HorizontalAlignment="Center">
                            <ui:TextBlock Text="Señas particulares"/>
                            <ui:TextBox Width="800" Height="110" Text="{Binding SenasParticularesVehiculo, Mode=TwoWay}"/>
                        </StackPanel>
                    </Grid>
                    
