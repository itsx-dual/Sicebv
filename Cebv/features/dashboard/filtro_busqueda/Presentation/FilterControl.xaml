                        </StackPanel>
                    </Grid>
                    
                    <Grid x:Name="MediaFiliacion" Visibility="Collapsed">
                        <WrapPanel>
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Estatura"/>
                                <ui:NumberBox Width="225" Text="{Binding Estatura, Mode=TwoWay}"
                                              MaxDecimalPlaces="0" Maximum="250" Minimum="30" PlaceholderText="Centímetros"
                                              SmallChange="1" Tag="Units" Icon="{ui:SymbolIcon Ruler24}"/>
                            </StackPanel>
                                
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Peso"/>
                                <ui:NumberBox Width="225" Text="{Binding Peso, Mode=TwoWay}"
                                              MaxDecimalPlaces="0" Maximum="250" Minimum="30" PlaceholderText="Kilogramos"
                                              SmallChange="1" Tag="Units"  />
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Complexion"/>
                                <ComboBox Width="225" SelectedItem="{Binding ComplexionSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Complexiones}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color piel"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorPielSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding ColoresPiel}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color ojos"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorOjosSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding ColoresOjos}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color cabello"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorCabelloSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding ColoresCabello}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Tamaño cabello"/>
                                <ComboBox Width="225" SelectedItem="{Binding TamanioCabelloSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding TamanosCabello}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Tipo cabello"/>
                                <ComboBox Width="225" SelectedItem="{Binding TipoCabelloSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding TiposCabello}"/>
                            </StackPanel>
                        </WrapPanel>
                    </Grid>
                    
                    <Grid x:Name="SeñasParticulares" Visibility="Collapsed">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1.2*" />
                            <ColumnDefinition />
                            <ColumnDefinition />
                        </Grid.ColumnDefinitions>
                        <Grid Grid.Column="0"
                              Grid.ColumnSpan="3">
                            <StackPanel>
                                <!-- Tipo de seña -->
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition />
                                        <ColumnDefinition />
                                    </Grid.ColumnDefinitions>
                                                                               
                                    <StackPanel>
                                        <!-- Region cuerpo -->
                                        <ui:TextBlock Text="Región del cuerpo:"
                                                      Margin="10,10,0,0" />
                                        <ComboBox Margin="5,0,5,0"
                                                                                   
                                                  SelectedValue="{Binding RegionCuerpoSelected, Mode=TwoWay}"
                                                  ItemsSource="{Binding RegionesCuerpo}" />
                                                                               
                                        
                                        <!-- Vista -->
                                        <ui:TextBlock Text="Vista:"
                                                      Margin="10,10,0,0" />
                                        <ComboBox Margin="5,0,5,0"
                                                                                   
                                                  SelectedValue="{Binding VistaSelected, Mode=TwoWay}"
                                                  ItemsSource="{Binding Vistas}" />
                                    </StackPanel>
                                                                     
                                    <StackPanel Grid.Column="1">
                                        <ui:TextBlock Text="Lado:"
                                                              Margin="10,10,0,0" />
                                        <ComboBox Margin="5,0,5,0"
                                                                                  
                                                  SelectedValue="{Binding LadoSelected, Mode=TwoWay}"
                                                  ItemsSource="{Binding Lados}" />
                                                                               
                                        <!-- Cantidad -->
                                        <ui:TextBlock Text="Cantidad:"
                                                      Margin="10,10,0,0" />
                                        <ui:NumberBox Margin="5,0,5,0"
                                                      Value="{Binding Cantidad,
                                                      Mode=TwoWay,
                                                      UpdateSourceTrigger=PropertyChanged}"
                                                      Tag="Number" Minimum="1" />
                                    </StackPanel>
                                </Grid>
                                                                               
                                <ui:TextBlock Text="Tipo de seña:"
                                              Margin="10,10,0,0" />
                                <ComboBox Margin="5,0,5,0"
                                                                           
                                          SelectedValue="{Binding TipoSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Tipos}" />
                                                                               
                                                                               
                                <!-- Descripcion -->
                                <ui:TextBlock Text="Descripción:"
                                              Margin="10,10,0,0" />
                                <TextBox Margin="10"
                                         VerticalAlignment="Stretch"
                                         Height="100"
                                         Text="{Binding Descripcion, Mode=TwoWay}"
                                         TextWrapping="Wrap" />
                            </StackPanel>
                        </Grid>         
                    </Grid>
                    
                    <Grid x:Name="VehiculosInvolucrados" Visibility="Collapsed">
