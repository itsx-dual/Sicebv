                                <ui:TextBlock Text="Descripcion"/>
                                <ui:TextBox Width="225" Text="{Binding HipotesisOficialDescripcion, Mode=TwoWay}"/>
                            </StackPanel>
                        </WrapPanel>
                    </Grid>
                    
                    <Grid x:Name="MediaFiliacion" Visibility="Collapsed">
                        <WrapPanel>
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Estatura"/>
                                <ui:TextBox Width="225" Text="{Binding Estatura, Mode=TwoWay}"/>
                            </StackPanel>
                                
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Peso"/>
                                <ui:TextBox Width="225" Text="{Binding Peso, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Complexion"/>
                                <ComboBox Width="225" SelectedItem="{Binding ComplexionSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding Complexiones}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color piel"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorPielSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding ColoresPiel}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color ojos"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorOjosSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding ColoresOjos}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Color cabello"/>
                                <ComboBox Width="225" SelectedItem="{Binding ColorCabelloSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding ColoresCabello}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Tamaño cabello"/>
                                <ComboBox Width="225" SelectedItem="{Binding TamanioCabelloSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding TamanosCabello}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Tipo cabello"/>
                                <ComboBox Width="225" SelectedItem="{Binding TipoCabelloSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding TiposCabello}"/>
                            </StackPanel>
                        </WrapPanel>
                    </Grid>
                    
                    <Grid x:Name="SeñasParticulares" Visibility="Collapsed">
                        <WrapPanel>
                            <ui:TextBlock Text="Pendiente" HorizontalAlignment="Center" FontTypography="Subtitle"/>
                            
