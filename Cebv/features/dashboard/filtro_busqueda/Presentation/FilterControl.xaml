                            <StackPanel Margin="0,0820,0">
                                <ui:TextBlock Text="Piercings"/>
                                <ui:TextBox Width="225" Text="{Binding Piercings}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,0820,0">
                                <ui:TextBlock Text="Protesis"/>
                                <ui:TextBox Width="225" Text="{Binding Protesis}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,0820,0">
                                <ui:TextBlock Text="Otra señas"/>
                                <ui:TextBox Width="225" Text="{Binding OtraSenas}"/>
                            </StackPanel>-->
                        </WrapPanel>
                    </Grid>
                </Grid>
                
                <Grid Grid.Row="2" HorizontalAlignment="Center">
                    <WrapPanel Orientation="Horizontal">
                        <ui:Button Content="Limpiar" Margin="10"
                                   Command="{Binding CleanCommand}"/>
                        <ui:Button Content="Buscar" Margin="10"  
                                   Command="{Binding CargarReportesCommand}"/>
                    </WrapPanel>
                </Grid>
            </Grid>
        </Expander>
    </Grid>
</UserControl>
