                                <ui:TextBlock Text="Grupo pertenencia:" />
                                <ComboBox Width="225" SelectedItem="{Binding GrupoPerteneciaSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding GruposPertenencia}" />
