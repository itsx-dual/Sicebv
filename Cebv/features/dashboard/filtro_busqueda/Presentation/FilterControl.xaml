                            </StackPanel>
                        </StackPanel>
                    </Grid>
                    
                    <Grid x:Name="DocumentosLegale" Visibility="Collapsed">
                        <WrapPanel Orientation="Horizontal">
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Tipo documento"/>
                                <ui:TextBox Width="225"
                                            Text="{Binding TipoDocumento, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0 8 20 0">
                                <ui:TextBlock Text="Numero documento"/>
                                <ui:TextBox Width="225"
                                            Text="{Binding NumeroDocumento, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0 8 20 0">
                                <ui:TextBlock Text="Donde radica"/>
                                <ui:TextBox Width="225"
                                            Text="{Binding DondeRadica, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0 8 20 0">
                                <ui:TextBlock Text="Nombre Servidor publico"/>
                                <ui:TextBox Width="225"
                                            Text="{Binding NombreServidorPublico, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0 8 0 0">
                                <ui:TextBlock Text="Fecha recepcion"/>
                                <DatePicker Width="225"
                                            SelectedDate="{Binding FechaRecepcion, Mode=TwoWay}"/>
                            </StackPanel>
                        </WrapPanel>
