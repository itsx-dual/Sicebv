                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Otro derecho humanol"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap" 
                                            Text="{Binding OtroDerechoHumano, Mode=TwoWay}"/>
                            </StackPanel>
                             
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Clasificacion Persona"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap" 
                                            Text="{Binding ClasificacionPersona, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Identidad resguardada"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap" 
                                            Text="{Binding OtroDerechoHumano, Mode=TwoWay}"/>
                            </StackPanel>
                        </WrapPanel>
                     </Grid>
        
                    <Grid x:Name="HechosDesaparicion" Visibility="Collapsed">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <WrapPanel>
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Fecha desaparicion"/>
                                <DatePicker Width="225"  IsTodayHighlighted="True" SelectedDateFormat="Short"
                                            SelectedDate="{Binding FechaDesaparicion, Mode=TwoWay}"/>
                            </StackPanel>
                                    
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Fecha desaparicion aproximada"/>
                                <DatePicker Width="225" IsTodayHighlighted="True" SelectedDateFormat="Short"
                                            SelectedDate="{Binding FechaDesaparicionAproximada, 
                                            Mode=TwoWay}"/>
                            </StackPanel>
                                
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Fecha desaparicion Cebv"/>
                                <DatePicker Width="225"  IsTodayHighlighted="True" SelectedDateFormat="Short"
                                            SelectedDate="{Binding FechaDesaparicionCebv, 
                                            Mode=TwoWay}"/>
                            </StackPanel>
                
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Hora desaparicion"/>
                                <ui:TextBox Width="225" Icon="{ui:SymbolIcon Clock24}" Tag="Time"
                                            Text="{Binding HoraDesaparicion, Mode=TwoWay}"/>
                            </StackPanel>
                                    
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Fecha percato"/>
                                <DatePicker Width="225"  IsTodayHighlighted="True" SelectedDateFormat="Short" 
                                            SelectedDate="{Binding FechaPercato, Mode=TwoWay}"/>
                            </StackPanel>
                                    
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Fecha percato CebV"/>
                                <DatePicker Width="225"  IsTodayHighlighted="True" SelectedDateFormat="Short"
                                            SelectedDate="{Binding FechaPercatoCebv, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Hora percato"/>
                                <ui:TextBox Width="225" Icon="{ui:SymbolIcon Clock24}" Tag="Time"
                                            Text="{Binding HoraPercato, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Amenaza"/>
                                <ComboBox Width="225"
                                          SelectedValue="{Binding AmenazaCambioComportamiento, Mode=TwoWay}"
                                          DisplayMemberPath="Key"
                                          SelectedValuePath="Value"
                                          ItemsSource="{Binding Opciones}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Personas mismo evento"/>
                                <ui:NumberBox Width="225" Tag="Number" Minimum="0"
                                            Text="{Binding PersonaMismoEvento, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Numero desapariciones previas"/>
                                <ui:NumberBox Width="225" Tag="Number" Minimum="0"
                                            Text="{Binding ContadorDesapariciones, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Hechos Desaparicion"/>
                                <ui:TextBox Width="225" Tag="Exclude" 
                                            Text="{Binding HechosDesaparicion, Mode=TwoWay}"/>
                            </StackPanel>
                        </WrapPanel>
                        <WrapPanel Orientation="Horizontal" Grid.Row="1">
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Descripcion amenazao"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap"
                                            Text="{Binding DescripcionAmenazaCambioComportamiento, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Situacion previa"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap"
                                            Text="{Binding SituacionPrevia, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="informacion relevante"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap"
                                            Text="{Binding InformacionRelevante, Mode=TwoWay}"/>
                            </StackPanel>
                            
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Sintesis desaparicion"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap"
                                            Text="{Binding SintesisDesaparicion, Mode=TwoWay}"/>
                            </StackPanel>
                        </WrapPanel>
                    </Grid>
                    
                    <Grid x:Name="HipotesisOficial" Visibility="Collapsed">
                        <WrapPanel>
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Hipotesis ofical"/>
                                <ComboBox Width="225" SelectedItem="{Binding HipotesisOficialSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding HipotesisOficial}"/>
                            </StackPanel>
                                
                            <StackPanel Margin="0,8,20,0">
                                <ui:TextBlock Text="Circunstancia"/>
                                <ComboBox Width="225" SelectedItem="{Binding HipotesisOficialCircunstanciaSelected, Mode=TwoWay}"
                                          ItemsSource="{Binding HipotesisOficialCircunstancia}"/>
                            </StackPanel>
                        </WrapPanel>
                        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                            <StackPanel Margin="0,8,0,0">
                                <ui:TextBlock Text="Descripcion"/>
                                <ui:TextBox Width="260" Height="112" TextWrapping="Wrap" 
                                            Text="{Binding HipotesisOficialDescripcion, Mode=TwoWay}"/>
